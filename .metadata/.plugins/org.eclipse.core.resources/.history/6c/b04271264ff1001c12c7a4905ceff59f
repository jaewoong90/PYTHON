class Animal:
    def __init__(self): #init은 자바의 생성자
        self.age = 1  #자바의 필드로 생각
    def happyBirth(self):
        self.age+=1
        
class Human(Animal):  
    def __init__(self):
        super().__init__()
        self.money = 10000
    
    def albamon(self):
        self.money += 1
        

h = Human()
print(h.money)
print(h.age)
h.happyBirth()
h.albamon()
print(h.money)
print(h.age)
